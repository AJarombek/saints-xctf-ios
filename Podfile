use_frameworks!

# The main iOS application
target 'SaintsXCTF' do
  pod 'ObjectMapper', '~> 3.5.1'
  pod 'BCryptSwift', '~> 1.1'
  pod 'PopupDialog', '~> 1.1.0'
  pod 'Charts', '~> 3.3.0'
  pod 'IQKeyboardManagerSwift', '~> 6.4.2'
end

# Unit tests for the iOS application
target 'SaintsXCTFTests' do
  pod 'ObjectMapper', '~> 3.5.1'
end
